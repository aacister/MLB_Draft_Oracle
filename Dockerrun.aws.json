{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "aacister/mlb-draft-oracle:1.0",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": 8080
    }
  ],
  "Logging": "/var/log/web.stdout.log",
  "Environment": {
    "PYTHONUNBUFFERED": "1",
    "PORT": "8080",
    "BRAVE_API_KEY": "${BRAVE_API_KEY}",
    "OPENAI_API_KEY": "${OPENAI_API_KEY}",
    "DB_URL": "${DB_URL}"
  }
}